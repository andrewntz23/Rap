start = album
album = element album {type, meta, song+}
meta = element meta {albumTitle, source, year, accessDate}
albumTitle = element albumTitle {"1999"|"B4.DA.$$"|"SummerKnights"|"Rejex"}
source = element source {text}
year = element year {text}
accessDate = element accessDate {text}
song = element song {title, production, stanza+}
title = attribute title {text}
production = attribute production {text}
stanza = element stanza {type, speaker, li+}
speaker = attribute speaker {"2pac" |  "ALa$ole" |  "ActionBronson" |  "CJFly" |  "CapitalSTEEZ" |  "Chronixx" |  "ChuckStrangers" |  "CollieBuddz" |  "DessyHinds" |  "DirtySanchez" |  "DyemondLewis" |  "ElleVarner" |  "Joey" |  "Joey BJtheChicagoKid" |  "JoeyBada$$" |  "JoeyBada$$ CJFly" |  "JoeyBada$$ KirkKnight" |  "Kieza" |  "KirkKnight" |  "MaverickSabre" |  "NYCkCaution" |  "Nas Youngster" |  "NyckCaution" |  "Pinky JoeyBada$$" |  "ProEra" |  "Raury" |  "Rokamouth" |  "Sample" |  "SaulWilliams" |  "SmokeDZA" |  "T'nahApex" |  "Unknown" |  "Unknown CapitalSTEEZ" |  "UnknownSoldier" |  "unknown" }
type = attribute type {"album"|"mixtape"|"verse"|"intro"|"bridge"|"hook"|"skit"|"interlude"|"outro"|"sample"}
li = element li {mixed {markup}
markup = (oldschool|success|women|fashion|struggle|food|money|hiphop|haters|guns|popculture|weed|sports|geo|group|AAVE|compound)
oldschool = element oldschool {mixed {markup}}
success = element success {mixed {markup}}
women = element women {mixed{markup} 
fashion = element fashion {mixed {markup}}
struggle = element struggle {mixed{markup}}
food = element food {mixed{markup}}
money = element money {mixed{markup}}
haters = element haters {mixed{markup}}
guns = element guns {mixed{markup}}
weed = element weed {mixed{markup}}
sports = element sports {mixed{markup}}
hiphop = element hiphop {mixed{ref, markup}}
ref = attribute ref {"Jay-Z"|"LilWayne"|"LupeFiasco"|"Tupac"|"Diddy"|"NotoriousBIG"|"50Cent"|"KanyeWest"|"NickiMinaj"|"JDilla"|"DragonBallZ"|"Batman"|"MurderSheWrote"|"DonCheadle"|"MaxKeeble"|"MojoJojo"|"Brooklyn"|"NewYork"|"NewMexico"|"Spain"|"EastCoast"|"Boston"}
popculture = element popculture {mixed {ref, markup}}
geo = element geo {mixed {ref, markup}}
group = element group {mixed {label, num, (alliteration|assonance|assimilation|consonance|repetition|rhyme|slantRhyme)+, markup}}
label = attribute label {text}
num = attribute num {xsd:int}
alliteration = attribute alliteration {xsd:int}
assonance = attribute assonance {xsd:int}
assimilation = attribute assimilation {xsd:int}
consonance = attribute consonance {xsd:int}
repetition = attribute repetition {xsd:int}
rhyme = attribute rhyme {xsd:int}
slantRhyme = attribute slantRhyme {xsd:int}
AAVE = element AAVE {mixed {field, type, (sound|position|word1|word2|word3|person|class)+, markup}}
compound= element compound {mixed {head, stem, type} markup}
field = attribute field {"syn"|"phon"}
type = attribute type {"deletion"|"contraction"|"tense"|"aspect"|"dropping"|"exo"|"endo"}
sound = attribute sound {text}
position = attribute position {"initial"|"middle"|"final"}
word1 = attribute word1 {text}
word2 = attribute word2 {text}
word3 = attribute word3 {text}
person = attribute person {text}
class = attribute class {"copula"|"preposition"}
head = attribute head {text}
stem = attribute stem {text}
#If you want to make a group variable for something you just use label=(blah|blah1|blah2|etc.) 
